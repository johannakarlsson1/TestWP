{
  "name": "wordpress-base",
  "description": "Wordpress base",
  "homepage": "https://stena.com/",
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "package",
      "package": {
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.6.3",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&t=5.6.3&k=b3JkZXJfaWQ9MTE1NDAwfHR5cGU9ZGV2ZWxvcGVyfGRhdGU9MjAxNy0wOS0yNyAxMDo1MDo0Nw=="
        },
        "require": {
          "composer/installers": "^1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "deliciousbrains/wp-migrate-db-pro",
        "type": "wordpress-plugin",
        "version": "1.8",
        "dist": {
            "type": "zip",
            "url": "https://deliciousbrains.com/dl/wp-migrate-db-pro-latest.zip?licence_key=445e21f4-f451-4f4c-bd0d-b560c7904017&site_url=thereef.se"
        },
        "require": {
            "composer/installers": "^1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "deliciousbrains/wp-offload-s3-pro",
        "type": "wordpress-plugin",
        "version": "1.5",
        "dist": {
          "type": "zip",
          "url": "https://deliciousbrains.com/dl/wp-offload-s3-pro-latest.zip?licence_key=a42ebc95-24aa-4813-a8e6-615e433938ab&site_url=thereef.se"
        },
        "require": {
          "composer/installers": "^1.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "ithemes/ithemes",
        "type": "wordpress-plugin",
        "version": "4.7.1",
        "dist": {
          "type": "zip",
          "url": "https://members.ithemes.com/packages/ithemes-security-pro.zip"
        },
        "require": {
          "composer/installers": "^1.0"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.4",
    "johnpbloch/wordpress": "4.9.1",
    "oscarotero/env": "^1.1.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "advanced-custom-fields/advanced-custom-fields-pro":"5.6.3",
    "deliciousbrains/wp-migrate-db-pro": "1.8",
    "deliciousbrains/wp-offload-s3-pro": "1.5",
    "wpackagist-plugin/amazon-web-services": "1.0.4",
    "wpackagist-plugin/better-wp-security": "6.7.0",
    "wpackagist-plugin/easy-wp-smtp": "1.3.1",
    "wpackagist-plugin/wp-fastest-cache": "0.8.7.5",
    "wpackagist-plugin/wordpress-seo": "5.8",
    "wpackagist-plugin/worker": "4.2.25",
    "wpackagist-plugin/wp-smushit": "2.7.6"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0.2"
  },
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "web/app/plugins/{$name}/": ["type:wordpress-plugin"],
      "web/app/themes/{$name}/": ["type:wordpress-theme"]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs"
    ]
  }
}
